{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxRequestBodySize": "50mb",
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/api/reservas/(.*)",
      "methods": ["OPTIONS"],
      "dest": "/server.js"
    },
    {
      "src": "/api/reservas/(.*)",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"],
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
